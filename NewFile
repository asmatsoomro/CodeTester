
CREATE TABLE promotions.promo_def (
  promo_id text,
  store_no text,
  type text,
  active text,
  start_date text,
  end_date text,
  original_description text,
  coupon_usage_limit text,
  extrabuck_coupon_amount double,
  extrabuck_offertype_code text,
  extrabuck_threshold_amount double,
  minimum_quantity double,
  minimum_spend_amount double,
  offer_buyquantity double,
  offer_getquantity double,
  offer_price text,
  offer_sale_amount double,
  offer_sale_qty double,
  percentage_dollaroff_amount double,
  percentage_offercap_amount double,
  reduced_amount double,
  regular_retail_indicator text,
  sale_offer_type text,
  secondary_price_amount double,
  adblock_number text,
  adpage_number text,
  adpromotype_number text,
  ad_version_code text,
  bopis_eligible_indicator text,
  order_level_indicator text,
  priority text,
  promo_disclaimer text,
  sot_disclaimer text,
  sku_list_text text,
  promo_type_code text,
  promo_active_indicator text,
  state text,
  offer_minimum_quantity int,
  bopis_item_quantity int,
  bopis_minimum_spend_amount int,
  shipping_method_code text,
  requested_coupon_count int,
  PRIMARY KEY ((promo_id, store_no))
);
